{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@sections/*": ["src/sections/*"],
      "@assets/*": ["src/assets/*"],
      "@services/*": ["src/services/*"],
      "@config/*": ["src/config/*"],
      "@utils/*": ["src/utils/*"]
    }

  }
}
